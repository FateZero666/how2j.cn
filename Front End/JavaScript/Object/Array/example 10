<html>
<head>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8">
</head>

<style>

</style>

<script>
function p(s){
 document.write(s);
 document.write("<br>");
}

var x=new Array(3,1,4,1,5,9,2,6);
p("使用sort排序前的数组是：" + x);
x.sort();
p("使用sort排序但未去重的数组是：" + x);
var temp=new Array();
temp.push(x[0]);
var length=x.length;
for(var i=0; i<length; ++i){
 if(x[i] != temp[temp.length - 1]){
   temp.push(x[i])
 }
}
p("使用sort排序后的数组是：" + temp);
</script>

<body>
 
</body>
</html>
