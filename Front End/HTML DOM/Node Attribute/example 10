<script>
function way1(){
 var i1=document.getElementById("input").value;
 var before=i1;
 alert(before);
 var after=parseInt(before);
 alert(after);
 if(after == before){
    alert("is int");
 }else{
    alert("not int");
 }
}
</script>

<body>
<input id="input" type="text"><br><br>
<button onclick="way1()">判断</button>
</body>
